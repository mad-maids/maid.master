{
  "commands": {
    "test": ["deno", "test", "--allow-all", "test.ts"],
    "run": ["deno", "run", "--allow-all", "main.ts"],
    "fmt": ["deno", "fmt"],
    "lint": ["deno", "lint"],
    "compile": [
      "deno",
      "compile",
      "--allow-all",
      "--output",
      "stacktion",
      "main.ts"
    ],
    "prt": [
      "prettier",
      "--config",
      ".prettierrc",
      "--write",
      "--check",
      "**/*.ts"
    ]
  }
}
